{
  "kind": "collectionType",
  "collectionName": "lab_submission_records",
  "info": {
    "singularName": "lab-submission-record",
    "pluralName": "lab-submission-records",
    "displayName": "Lab_Submission_Record",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Date": {
      "type": "datetime"
    },
    "Lab_name": {
      "type": "string"
    },
    "Quality_grade": {
      "type": "enumeration",
      "enum": [
        "Grade A",
        "Grade B",
        "Grade C",
        "Grade D",
        "Grade F"
      ]
    },
    "Submission_status": {
      "type": "enumeration",
      "enum": [
        "Completed",
        "Pending",
        "Failed",
        "Waiting"
      ]
    },
    "Report": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "batch": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::batch.batch",
      "inversedBy": "lab_submission_records"
    },
    "harvest_record": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::harvest-record.harvest-record",
      "inversedBy": "lab_submission_record"
    }
  }
}
