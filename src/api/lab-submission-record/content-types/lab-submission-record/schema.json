  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Date": {
      "type": "datetime"
    },
    "Quality_grade": {
      "type": "enumeration",
      "enum": [
        "Grade A",
        "Grade B",
        "Grade C",
        "Grade D",
        "Grade F"
      ]
    },
    "Submission_status": {
      "type": "enumeration",
