{
  "kind": "collectionType",
  "collectionName": "harvest_records",
  "info": {
    "singularName": "harvest-record",
    "pluralName": "harvest-records",
    "displayName": "Harvest_Record",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Date": {
      "type": "datetime"
    },
    "yleld": {
      "type": "decimal"
    },
    "quality_grade": {
      "type": "enumeration",
      "enum": [
        "Grade A",
        "Grade B",
        "Grade C",
        "Grade D",
        "Grade F"
      ]
    },
    "Note": {
      "type": "text"
    },
    "Curcumin_quality": {
      "type": "decimal"
    },
    "Yleld_unit": {
      "type": "enumeration",
      "enum": [
        "kg",
        "g"
      ]
    },
    "batch": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::batch.batch",
      "inversedBy": "harvest_records"
    },
    "Harvest_status": {
      "type": "enumeration",
      "enum": [
        "Completed",
        "Pending",
        "Failed",
        "Waiting"
      ]
    },
    "lab_submission_record": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::lab-submission-record.lab-submission-record",
      "mappedBy": "harvest_record"
    },
    "Submission_status": {
      "type": "enumeration",
      "enum": [
        "Completed",
        "Pending",
        "Failed",
        "Waiting"
      ]
    },
    "factory_submission": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::factory-submission.factory-submission",
      "inversedBy": "harvest_records"
    },
    "kamincal_sample_name": {
      "type": "string"
    },
    "kamincal_plant_weight": {
      "type": "decimal"
    },
    "kamincal_solvent_volume": {
      "type": "decimal"
    },
    "kamincal_average_od": {
      "type": "decimal"
    },
    "kamincal_concentration": {
      "type": "decimal"
    },
    "kamincal_number_of_replications": {
      "type": "integer"
    },
    "kamincal_first_time": {
      "type": "decimal"
    },
    "kamincal_analytical_instrument": {
      "type": "enumeration",
      "enum": [
        "UV-Vis",
        "LED"
      ]
    },
    "kamincal_second_time": {
      "type": "decimal"
    },
    "kamincal_curcuminoid_content": {
      "type": "enumeration",
      "enum": [
        "Pass",
        "Fail"
      ]
    },
    "kamincal_curcuminoid_percentage": {
      "type": "decimal"
    },
    "kamincal_third_time": {
      "type": "decimal"
    },
    "labor_cost": {
      "type": "decimal",
      "min": 0,
      "default": 0
    },
    "equipment_cost": {
      "type": "decimal",
      "min": 0,
      "default": 0
    },
    "total_harvest_cost": {
      "type": "decimal",
      "min": 0,
      "default": 0
    },
    "Method": {
      "type": "string"
    },
    "Result_type": {
      "type": "string"
    }
  }
}
