    "user_documentId": {
      "type": "string"
    },
    "waste_quantity": {
      "type": "decimal"
    },
    "operator_processor": {
      "type": "string"
    },
    "output_unit": {
      "type": "enumeration",
      "enum": [
        "kg",
        "packs",
        "boxes"
      ]
    },
    "export_factory_history": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::export-factory-history.export-factory-history",
      "mappedBy": "factory_processing"
    }
